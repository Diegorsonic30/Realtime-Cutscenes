{
  "Groups": [
    {
      "Name": "Main",
      "DisplayName": "Realtime Cutscenes - Optional Scenes",
      "Elements": [
        {
          "Name": "IncludeDir1",
          "DisplayName": "Starfall Events",
          "Description": ["Determines whether to play the Starfall events in Realtime or leave them pre-rendered."],
          "Type": "rt se",
          "MinValue": null,
          "MaxValue": null,
          "DefaultValue": "rt se"
        },
        {
          "Name": "IncludeDir2",
          "DisplayName": "Rhea Island Scenes",
          "Description": [
            "Determines whether some Rhea Island scenes are Realtime or pre-rendered."],
          "Type": "rt rs",
          "MinValue": null,
          "MaxValue": null,
          "DefaultValue": "rt rs"
        },
        {
          "Name": "IncludeDir3",
          "DisplayName": "Rework Supreme Support",
          "Description": [
            "The Holoska mod 'Rework Supreme' is incompatible with a RealTime Custscene scene, the idea of this configuration file is to make that mod compatible!"],
          "Type": "rt Rework Support",
          "MinValue": null,
          "MaxValue": null,
          "DefaultValue": "rt Rework Support" 
        }
      ]
    }
  ],
  "Enums": {
    "rt se": [
      {
        "DisplayName": "Activated",
        "Value": "rt se",
        "Description": ["Uses the Realtime Starfall events."]
      },
      {
        "DisplayName": "Disabled",
        "Value": "empty",
        "Description": ["Uses Pre-Rendered Starfall Events."]
      }
    ],
    "rt rs": [
      {
        "DisplayName": "Activated",
        "Value": "rt rs",
        "Description": ["Use the Rhea Island a RealTime scenes!"]
      },
      {
        "DisplayName": "Disabled",
        "Value": "empty",
        "Description": ["Uses some pre-rendered scenes on Rhea Island."]
      }
    ],
    "rt Rework Support": [
      {
        "DisplayName": "Disabled",
        "Value": "empty",
        "Description": ["Use the scene that is incompatible with the 'Rework Supreme' mod."]
      },
      {
        "DisplayName": "Activated", 
        "Value": "rt Rework Support",
        "Description": ["Disables the scene, and makes the 'Rework Supreme' mod compatible."]
      }
    ]
  },
  "IniFile": "mod.ini"
}
